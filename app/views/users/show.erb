<!doctype html>
<html>
  <head>
    <title>Fwitter</title>
  </head>
  <body>
    <h2>Welcome, <%= @user.username %> </h2>

    <% Tweet.each do |tweet| %>
      <% if tweet.user_id == @user.id %>
      <p><%= tweet.content %></p>
      <% end %>
    <% end %>

    <p>Create Tweet Here</p>

  </body>
</html>
